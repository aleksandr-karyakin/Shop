    <tag>
        <tag-name>datatable</tag-name>
        <source>Datatable.xhtml</source>
        <attribute>
            <description>List of orders</description>
            <name>orderList</name>
            <type>java.util.List</type>
        </attribute>
    </tag>
    <tag>
        <tag-name>itemList</tag-name>
